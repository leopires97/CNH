<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:TabbedPageSample;assembly=TabbedPage"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom"
            x:Class="TabbedPageSample.AppTabbedPage">

    <NavigationPage Title="Home" IconImageSource="{StaticResource TabOne}">
        <x:Arguments>
            <local:MainPage Title="Home" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Ajuda" IconImageSource="{StaticResource TabThree}">
        <x:Arguments>
            <local:Ajuda Title="Ajuda" />
        </x:Arguments>
    </NavigationPage>

</TabbedPage>
